<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="RevEngEFrame">
		<title>RevEngE (use the mouse wheel to change the gradient)</title>
		<size>1331,766</size>
		<fg>#000000</fg>
		<id_arg>0</id_arg>
		<handler function="OnClose" entry="EVT_CLOSE" />
		<object class="wxStaticBitmap" name="ID_STATICBITMAP1" variable="StaticBitmap1" member="yes">
			<minsize>0,0</minsize>
			<maxsize>0,0</maxsize>
			<style>wxFULL_REPAINT_ON_RESIZE</style>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="ID_MENU_LOAD1" variable="MenuItem5" member="yes">
					<label>Load image&amp;1</label>
				</object>
				<object class="wxMenuItem" name="ID_MENU_LOAD2" variable="MenuItem6" member="yes">
					<label>Load image&amp;2</label>
				</object>
				<object class="wxMenuItem" name="ID_MENU_LATEST" variable="MenuItem3" member="yes">
					<label>&amp;Load latest files</label>
					<enabled>0</enabled>
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxImage" variable="Image1" member="yes" />
		<object class="wxImage" variable="Image2" member="yes" />
		<object class="wxImage" variable="Image3" member="yes" />
		<object class="wxFileDialog" variable="FileDialog1" member="yes">
			<message>Select file</message>
		</object>
	</object>
</wxsmith>
